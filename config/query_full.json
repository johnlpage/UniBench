{
  "testClassName": "com.mongodb.solcon.tests.QueryTest",
  "numberOfThreads": 48,
  "database": "unibench",
  "collection": "query",
  "initialDocsToInsert": 5000000,
  "groupSize": 400,
  "writeBatchSize": 1000,
  "docSizeKB": 4.0,
  "testTimeSecs": 300,
  "warmup": true,
  "variants": [
    {
      "comment": "1 document, 1 term,  cached, indexed.",
      "nQueries": 4000000,
      "query": {
        "_id": "RANDINT(1,=$initialDocsToInsert/500)"
      },
      "limit": 1
    },
    {
      "comment": "1 document, not cached, indexed.",
      "nQueries": 2000000,
      "query": {
        "_id": "RANDINT(1,=$initialDocsToInsert)"
      },
      "limit": 1
    },
    {
      "comment": "100 documents,1 term, cached, indexed ",
      "nQueries": 2000000,
      "query": {
        "group": "RANDINT(1,100)"
      },
      "limit": 100
    },
    {
      "comment": "100 documents, 1 term, not cached, indexed",
      "nQueries": 5000,
      "query": {
        "group": "RANDINT(1,=$initialDocsToInsert/400)"
      },
      "limit": 100
    },
    {
      "comment": "20 Documents, 1 term, cached",
      "nQueries": 500000,
      "query": {
        "group": "RANDINT(1,100)"
      },
      "limit": 20
    },
    {
      "comment": "20 Documents, 380 skipped, 1 term, cached",
      "nQueries": 500000,
      "query": {
        "group": "RANDINT(1,100)"
      },
      "limit": 20,
      "skip": 380
    },
    {
      "comment": "20 documents, 380 range skipped, 1 term, cached",
      "nQueries": 100000,
      "query": {
        "group": "RANDINT(1,100)",
        "group_seq_i": {
          "$gte": 380
        }
      },
      "limit": 20
    },
    {
      "comment": "20 documents, 1 term, not cached",
      "nQueries": 5000,
      "query": {
        "group": "RANDINT(1,=$initialDocsToInsert/400)"
      },
      "limit": 20
    },
    {
      "comment": "20 documents, 380 skipped, 1 term, not cached",
      "nQueries": 5000,
      "query": {
        "group": "RANDINT(1,=$initialDocsToInsert/400)"
      },
      "limit": 20,
      "skip": 380
    },
    {
      "comment": "20 documents, 380 range skipped, 1 term, not cached",
      "nQueries": 5000,
      "query": {
        "group": "RANDINT(1,=$initialDocsToInsert/400)",
        "group_seq_i": {
          "$gte": 380
        }
      },
      "limit": 20
    },
    {
      "comment": "1 Document, 2 terms, partial index, not cached",
      "nQueries": 1000,
      "query": {
        "group": "RANDINT(1,=$initialDocsToInsert/400)",
        "group_seq": "RANDINT(1,400)"
      },
      "limit": 1
    },
    {
      "comment": "1 Document, 2 terms, compound index,  cached",
      "nQueries": 5000,
      "query": {
        "group": "RANDINT(1,=$initialDocsToInsert/400)",
        "group_seq_i": "RANDINT(1,400)"
      },
      "limit": 1
    },
    {
      "comment": "100 Documents, 100 Terms, indexed, cached",
      "nQueries": 1000,
      "query": {
        "_id": {
          "$in": "RANDINTLIST(1,10000,100)"
        }
      },
      "limit": 100
    },
    {
      "comment": "100 Documents, 100 Terms, indexed, not cached",
      "nQueries": 1000,
      "query": {
        "_id": {
          "$in": "RANDINTLIST(1,=$initialDocsToInsert,100)"
        }
      },
      "limit": 100
    }
  ]
}

