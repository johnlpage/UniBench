      "query": {
        "group": "RANDINT(1,25)"
      },
      "limit": 20
    },
    {
      "order": 6,
      "comment": "20 Documents, 380 skipped, 1 term, cached",
      "nQueries": 4000000,
      "query": {
        "group": "RANDINT(1,25)"
      },
      "limit": 20,
      "skip": 380
    },
    {
      "order": 7,
      "comment": "20 documents, 380 range skipped, 1 term, cached",
      "nQueries": 4000000,
      "query": {
        "group": "RANDINT(1,25)",
