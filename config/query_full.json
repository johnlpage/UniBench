{
  "testClassName": "com.mongodb.solcon.tests.QueryTest",
  "numberOfThreads": 48,
  "database": "unibench",
  "collection": "query",
  "initialDocsToInsert": 60000000,
  "groupSize": 400,
  "writeBatchSize": 1000,
  "docSizeKB": 1.0,
  "testTimeSecs": 300,
  "warmup": true,
  "variants": [
    {
      "order": 1,
      "comment": "1 document, 1 term,  cached, indexed.",
      "nQueries": 4000000,
      "query": {
        "_id": "RANDINT(1,100000)"
      },
      "limit": 1
    },
    {
      "order": 2,
      "comment": "1 document, not cached, indexed.",
      "nQueries": 4000000,
      "query": {
        "_id": "RANDINT(1,60000000)"
      },
      "limit": 1
    },
    {
      "order": 3,
      "comment": "100 documents,1 term, cached, indexed ",
      "nQueries": 4000000,
      "query": {
        "group": "RANDINT(1,25)"
      },
      "limit": 100
    },
    {
      "order": 4,
      "comment": "100 documents, 1 term, not cached, indexed",
      "nQueries": 4000000,
      "query": {
        "group": "RANDINT(1,150000)"
      },
      "limit": 100
    },
    {
      "order": 5,
      "comment": "20 Documents, 1 term, cached",
